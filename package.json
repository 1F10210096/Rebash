{
  "name": "next-frourio-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "notios": "notios dev",
    "dev": "run-p dev:*",
    "dev:client": "next dev -p 3000",
    "dev:hcm": "hcm 'src/**/*.module.css' -w",
    "hcm": "hcm 'src/**/*.module.css'",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:path": "pathpida --ignorePath .gitignore -s -w",
    "build": "npm run generate && next build",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:path": "pathpida --ignorePath .gitignore -s",
    "lint": "run-p lint:js lint:prettier lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.js --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.ts{,x}\"",
    "lint:fix": "npm run lint:js -- --fix && prettier --write \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore && npm run lint:style -- --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:updateSnapshot": "vitest -u",
    "pretypecheck": "npm run generate && npm run generate --prefix server && npm run hcm",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "axios": "^0.27.2",
    "firebase": "^9.15.0",
    "jotai": "^1.13.0",
    "next": "^13.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/node": "^17.0.18",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "happy-css-modules": "^2.1.1",
    "node-fetch": "2",
    "notios": "^0.4.0",
    "pathpida": "^0.20.1",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.1",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.22.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
