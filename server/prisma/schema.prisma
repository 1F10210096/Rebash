datasource db {
  provider = "postgresql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        String   @id
  label     String
  done      Boolean
  createdAt DateTime
}

model Room {
  roomId    String   @id
  createdAt DateTime
}

model user {
  userId String @id
  comment   String
  roomIdasse String[]
}
